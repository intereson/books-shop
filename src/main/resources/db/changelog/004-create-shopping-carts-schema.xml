<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd">
    <changeSet id="004-create-shopping-carts-schema" author="Sergey Lukashonak">
        <createTable tableName="shopping_carts">
            <column name="id_shopping_cart"
                    type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="sum_price"
                    type="double">
            </column>
        </createTable>
        <insert tableName="shopping_carts">
            <column name="id_shopping_cart">1</column>
            <column name="sum_price">0.0</column>
        </insert>
        <insert tableName="shopping_carts">
            <column name="id_shopping_cart">2</column>
            <column name="sum_price">0.0</column>
        </insert>
    </changeSet>
</databaseChangeLog>